{
  "expo": {
    "name": "Marvel Oldies",
    "description": "A quick catalog for marvel old characters",
    "slug": "moapp",
    "privacy": "public",
    "sdkVersion": "26.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-moapp.png",
    "splash": {
      "image": "./assets/img/Splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff1e28"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jes.moapp"
    },
    "android": {
      "package": "com.jes.moapp"
    }
  }
}
